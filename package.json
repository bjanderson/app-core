{
  "name": "jspm-starter",
  "author": "",
  "version": "1.0.0",
  "description": "This repository is a base for different web application starters.",
  "license": "MIT",
  "homepage": "https://github.com/bjanderson/app-core/tree/jspm-starter",
  "main": "",
  "scripts": {
    "build": "gulp build",
    "postinstall": "jspm install",
    "serve:dist": "google-chrome http://localhost:20080 && NODE_ENV=dist nodejs server/app.js",
    "start": "gulp serve",
    "test": "karma start",
    "test:watch": "npm test -- --no-single-run --auto-watch",
    "test:cover": "istanbul cover _mocha --require test/setup.js -- ./test --compilers js:babel-core/register --recursive -R spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bjanderson/app-core.git"
  },
  "bugs": {
    "url": "https://github.com/bjanderson/app-core/issues"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-preset-es2015": "^6.6.0",
    "browser-sync": "^2.11.1",
    "chai": "^3.5.0",
    "express": "^4.13.4",
    "gulp": "^3.9.1",
    "gulp-nodemon": "^2.0.6",
    "gulp-replace": "^0.5.4",
    "gulp-standard": "^6.0.5",
    "isparta": "^4.0.0",
    "istanbul": "github:gotwarlost/istanbul#source-map",
    "jspm": "^0.16.31",
    "karma": "^0.13.22",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "github:douglasduteil/karma-coverage#next",
    "karma-jspm": "^2.0.2",
    "karma-mocha": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.24",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.5",
    "wrench": "^1.5.8"
  },
  "jspm": {
    "directories": {
      "baseURL": "client"
    },
    "configFile": "client/jspm-config.js",
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "__dirname",
      "describe",
      "it",
      "expect"
    ]
  }
}
